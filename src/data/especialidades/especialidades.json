    "cards": [
        {
            "title": "Cardiologia",
            "details": "É a especialidade médica que se destaca pelo diagnóstico e tratamento das doenças do coração e restantes componentes do sistema circulatório.",
            "btntext": "Saber Mais",
            "link": "/especialidades/cardiologia"
        },
        {
            "title": "Cirurgia Plástica",
            "details": "A Cirurgia Plática tem como objetivo a modificação resconstrutiva de uma parte do corpo com motivo médico ou estético.",
            "btntext": "Saber Mais",
            "link": "/especialidades/cirurgiaplastica"
        },
        {
            "title": "Clínica Geral",
            "details": "A Clínica Geral é especialidade médica que tem como fundamento o primeiro contacto do paciente com os cuidados primários de saúde.",
            "btntext": "Saber Mais",
            "link": "/especialidades/clinicageral"
        },
        {
            "title": "Dermatologia",
            "details": "A Dermatologia é a especialidade da medicina que se ocupa das doenças que afetam o maior orgão do corpo humano, a pele.",
            "btntext": "Saber Mais",
            "link": "/especialidades/dermatologia"
        },
        {
            "title": "Fisitatria",
            "details": "A Fisiatria é a área da medicina que se dedica à recuperação da função física e cognitiva que pode estar alterada por diversos tipos de doenças.",
            "btntext": "Saber Mais",
            "link": "/especialidades/fisitatria"
        },
        {
            "title": "Fisioterapia",
            "details": "É uma área que estuda, diagnostica, previne e trata disfunções cinético funcionais decorrentes de orgãos ou sistemas do corpo humano.",
            "btntext": "Saber Mais",
            "link": "/especialidades/fisioterapia"
        },
        {
            "title": "Ginecologia-Obstetricia",
            "details": "Para além de acompanhar a gravidez e o parto, esta é a especialidade que estuda as patologias do aparelho reprodutor feminino, tais como o cancro, infertilidade. ",
            "btntext": "Saber Mais",
            "link": "/especialidades/ginecologiaobstetricia"
        },
        {
            "title": "Medicina Interna",
            "details": "A Medicina Interna é a especialidade que aborda o estado de saúde do paciente no seu todo, tendo em conta os vários distúrbios que podem afetar o organismo do paciente.",
            "btntext": "Saber Mais",
            "link": "/especialidades/medicinainterna"
        },
        {
            "title": "Neurologia",
            "details": "A Neurologia define-se como a especialidade médica que trata das disfunções estruturais do sistema nervoso.",
            "btntext": "Saber Mais",
            "link": "/especialidades/neurologia"
        },
        {
            "title": "Nutrição",
            "details": "Área clínica que orienta de forma individual ou em grupo, pessoas que necessitem de dieta alimentar específica, como os casos de excesso de peso, diabéticos, hipertensos, etc.",
            "btntext": "Saber Mais",
            "link": "/especialidades/nutricao"
        }
    ],
    "cardsparttwo": [
        {
            "title": "Ortopedia",
            "details": "É a especialidade que se dedica ao estudo e ao tratamento das deformações, congénitas ou adquiridas, dos ossos, das articulações, dos músculos e dos tendões.",
            "btntext": "Saber Mais",
            "link": "/especialidades/ortopedia"
        },
        {
            "title": "Otorrinolaringologia",
            "details": "Especialidade da medicina que se ocupa do diagnóstico e tratamento das doenças da cabeça e pescoço, especialmente ouvido, nariz e garganta.",
            "btntext": "Saber Mais",
            "link": "/especialidades/otorrinolaringologia"
        },
        {
            "title": "Pediatria",
            "details": "A Pediatria é a área da medicina que trata o ser humano desde o nascimento até à idade dos 18 anos.",
            "btntext": "Saber Mais",
            "link": "/especialidades/pediatria"
        },
        {
            "title": "Pneumologia",
            "details": "A Pediatria é a área da medicina que trata o ser humano desde o nascimento até à idade dos 18 anos.",
            "btntext": "Saber Mais",
            "link": "/especialidades/pneumologia"
        },
        {
            "title": "Podologia",
            "details": "A Podologia é uma área da saúde que visa o diagnóstico, tratamento e prevenção de doenças relacionadas com os pés e as repercussões que daí podem advir para o corpo humano.",
            "btntext": "Saber Mais",
            "link": "/especialidades/podologia"
        },
        {
            "title": "Psicologia",
            "details": "A Psicologia é a área da saúde que estuda os processos mentais e do comportamento do ser humano e a sua interação com o ambiente físico e social.",
            "btntext": "Saber Mais",
            "link": "/especialidades/psicologia"
        },
        {
            "title": "Psiquiatria",
            "details": "A Psiquiatria é a área da medicina que lida com prevenção, diagnóstico e tratamento das diferentes perturbações mentais, sejam elas de origem orgânica ou funcional.",
            "btntext": "Saber Mais",
            "link": "/especialidades/psiquiatria"
        },
        {
            "title": "Terapia da Fala",
            "details": "É a área da saúde responsável pelo estudo, prevenção e tratamento das alterações da comunicação, desde a compreensão e expressão da linguagem oral, à leitura e escrita, etc.",
            "btntext": "Saber Mais",
            "link": "/especialidades/terapiafala"
        },
        {
            "title": "Terapia Ocupacional",
            "details": "O principal objetivo da Terapia Ocupacional é capacitar as pessoas para participarem com sucesso em atividades da vida diária nos diversos ambientes bem como na comunidade.",
            "btntext": "Saber Mais",
            "link": "/especialidades/terapiaocupacional"
        }
    ],
